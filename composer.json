{
    "name": "noctis/xtable",
    "type": "library",
    "description": "Simple wrapper for PHPSpreadsheet",
    "keywords": ["excel","phpexcel","xtable","wrapper","phpspreadsheet"],
    "homepage": "https://github.com/Noctis/xtable",
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Czejgis",
            "email": "lucas.czejgis@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-mbstring": "*",
        "phpoffice/phpspreadsheet": "^1.15"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.1",
        "vimeo/psalm": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Noctis\\XTable\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
